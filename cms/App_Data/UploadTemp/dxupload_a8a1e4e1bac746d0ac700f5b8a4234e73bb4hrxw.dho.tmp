SELECT 
lo.ObjId AS CustObjId
--,ac.Name AccountName
,lh.DocumentNumber
,lo.LocationTypeLId
,lo.Name AS CustName
,prh.DocumentTypeLId
,prh.InvoiceNumber
,prh.TotalValueForeign
,prh.AmountPaid
,prh.AmountDue
 FROM Location lo
 JOIN VwPaymentReceiptsToMatch prh 
 ON lo.ObjId = prh.CustomerLocationId
 JOIN PaymentHeader ph ON prh.InvoiceNumber = ph.InvoiceNumber
 JOIN LogisticsDocumentHeader lh ON lh.ObjId = ph.LogisticsDocumentHeaderId
 WHERE prh.DocumentTypeLId IN ('CIFP', 'CIPP', 'CIPR', 'CISH', 'CCN')
